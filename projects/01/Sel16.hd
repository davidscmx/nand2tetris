/**
 * 16-bit multiplexor: 
 * for i = 0..15 out[i] = a[i] if sel == 0 
 *                        b[i] if sel == 1
 */

CHIP MyChip{
    IN sel;
    OUT out;

    PARTS:
    And(a=sel, b=true, out=out);
}

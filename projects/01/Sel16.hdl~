/**
 * Helper chip to turn 1-bit bus length into 16-bit length.
 * Author: David Sosa
 * David 
 */

CHIP Sel16 {
    IN sel;
    OUT out[16];

    PARTS:
    And(a=sel, b=true, out=out[0]);
    And(a=sel, b=true, out=out[1]);
    And(a=sel, b=true, out=out[2]);
    And(a=sel, b=true, out=out[3]);
    And(a=sel, b=true, out=out[4]);
    And(a=sel, b=true, out=out[5]);
    And(a=sel, b=true, out=out[6]);
    And(a=sel, b=true, out=out[7]);
    And(a=sel, b=true, out=out[8]);
    And(a=sel, b=true, out=out[9]);
    And(a=sel, b=true, out=out[10]);
    And(a=sel, b=true, out=out[11]);
    And(a=sel, b=true, out=out[12]);
    And(a=sel, b=true, out=out[13]);
    And(a=sel, b=true, out=out[14]);
    And(a=sel, b=true, out=out[15]);        
}
